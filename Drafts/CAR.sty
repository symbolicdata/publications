\newcommand{\Abschnitt}[3]{{
 %\vspace*{5mm}\mbox{\textsc{primes}}
     \section*{\Large\bf\hrule\vspace{1mm}
      \centerline{#1}}\vspace{-2.5mm}\hrule}
   \label{#3}
  \addcontentsline{toc}{section}{\normalsize \bf #2 }
   \vspace*{4mm}
 }

\newcommand{\UnterAbschnitt}[3]{
  \vspace{5mm}
\subsection*{\Large \bf #1}\label{#3}
  \addcontentsline{toc}{subsection}{\normalsize \sl #2}
\vspace{4mm}}

\newcommand{\AufsatzOhneFoto}[6]{%
\vspace{3mm}
  \begin{center}
    \begin{minipage}[b]{120mm}
      {\Large\bfseries #1}  \\[1em]
      {\bfseries #5}  \\[1em]
      \href{mailto:#6}{\texttt{#6}}
      \end{minipage}
  \end{center}
  \addcontentsline{toc}{subsection}{\normalsize \sl #2
\upshape (#3)}
  \label{#4}
  \setcounter{satz}{0}}

\newcommand{\Aufsatz}[7]{%
\vspace{3mm}
  \begin{center}
    \begin{minipage}[b]{120mm}
      {\Large\bfseries #1}  \\[1em]
      {\bfseries #5}  \\[1em]
      \href{mailto:#7}{\texttt{#7}}
      \end{minipage}
    \hspace*{1em}
    \includegraphics[height=50mm]{#6}
  \end{center}
  \addcontentsline{toc}{subsection}{\normalsize \sl #2
\upshape (#3)}
  \label{#4}
  \setcounter{satz}{0}}

\newcommand{\AufsatzZwei}[9]{%
  \vspace{3mm}
  \begin{center}
    \begin{minipage}[b]{104mm}
      {\Large\bfseries #1}  \\[1em]
      {\bfseries #5}  \\[1em]
      \href{mailto:#8}{\texttt{#8}}\\
      \href{mailto:#9}{\texttt{#9}}
    \end{minipage}
    \hspace*{-10mm}
    \includegraphics[height=50mm]{#6}\hspace*{2mm}\includegraphics[height=50mm]{#7}\hspace*{-10mm}
  \end{center}
  \addcontentsline{toc}{subsection}{\normalsize \sl #2\newline\upshape (#3)}
 \label{#4}
 \setcounter{satz}{0}}

